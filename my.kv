<MainWindow>

    name: "main"
    scroll_grid:scroll_grid

    FloatLayout:
        orientation:'vertical'
        cols:1
        rows:2
        size: root.width, root.height

        Button:
            size_hint: 0.5,0.15
            pos_hint: {"x":0,"top":1}
            id: kanallar
            background_down:""
            background_color: 0.8, 0.8, 0.6, 1
            text: "Kanal\nAyarlari"
            font_size: 30
            on_release:
                app.root.current = "kanallar"
                root.manager.transition.direction = "left"

        Button:
            size_hint: 0.5,0.15
            pos_hint: {"x":0.5,"top":1}
            id: kanallar
            background_down:""
            background_color: 0.5, 0.8, 0.8, 1
            text: "Yenile"
            font_size: 40
            on_release:
                root.refresh()


        ScrollView:
            pos_hint: {"x":0,"top":0.85}
            size_hint:1,0.85
            do_scroll_x: False
            do_scroll_y: True
            bar_width: 40
            bar_height:
            bar_margin:2
            scroll_type: ['bars','content']

            GridLayout:
                id:scroll_grid
                orientation: "vertical"
                size_hint_y: None
                height: self.minimum_height
                row_default_height: 90
                cols:3







